{
    "name": "kayrunm/php-adventofcode",
    "description": "A PHP solution wrapper for Advent of Code.",
    "keywords": ["advent of code", "aoc", "laravel zero"],
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Kieran Marshall",
            "email": "kieranm93@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.4",
        "illuminate/http": "^8.0",
        "laravel-zero/framework": "^8.8"
    },
    "require-dev": {
        "mockery/mockery": "^1.4.3",
        "pestphp/pest": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": ["adventofcode"]
}
